{
    "name": "pkboom/laravel-tinker-on-vscode",
    "description": "Tinker on vscode",
    "keywords": [
        "pkboom",
        "TinkerOnVscode"
    ],
    "homepage": "https://github.com/pkboom/laravel-tinker-on-vscode",
    "license": "MIT",
    "authors": [
        {
            "name": "Keunbae Park",
            "email": "ppotpo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/tinker": "^2.6",
        "pkboom/file-watcher": "^0.1.3",
        "react/event-loop": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Pkboom\\TinkerOnVscode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pkboom\\TinkerOnVscode\\TinkerOnVscodeServiceProvider"
            ]
        }
    }
}